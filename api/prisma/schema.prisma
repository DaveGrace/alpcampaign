datasource DS {
  // optionally set multiple providers
  // example: provider = ["sqlite", "postgresql"]
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Post {
id Int @id @default(autoincrement())
title String
body String
createdAt DateTime @default(now())
}
model Mem {
  id             Int    @id @default(autoincrement())
  email          String @unique
  phone          Int?
  name           String
  street_address String
  suburb         String
  state          String
  postcode       Int
}

model Org {
  id             Int    @id @default(autoincrement())
  email          String @unique
  phone          Int?
  name           String
  street_address String
  suburb         String
  state          String
  postcode       Int
}
